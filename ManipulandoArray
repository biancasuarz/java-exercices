package testes;

import java.util.ArrayList;
import java.util.Collections;

public class ManipulandoArray {

    public static void main(String[] args) {

        ArrayList<Double> notas = new ArrayList<>();

        notas.add(7.0);
        notas.add(8.0);
        notas.add(9.0);
        notas.add(10.0);

        System.out.println(notas.toString());

    }
}
